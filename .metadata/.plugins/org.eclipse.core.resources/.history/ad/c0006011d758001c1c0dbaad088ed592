		case 1:
			System.out.println("wear new clothes");
			
		case 2:
			System.out.println("hungry");
			
		case 3:
			System.out.println("climb mountains");
			
		case 4:
			System.out.println("watch TV");
			
		case 5:
			System.out.println("dance");
			
		case 6:
			System.out.println("go to happy");
	}
		
